<?xml version="1.0" encoding="iso-8859-1"?>
<odoo>   
    <record id="wikucourse_coursecategory_view_tree" model="ir.ui.view">
        <field name="name">wikucourse_coursecategory_view_tree</field>
        <field name="model">wikucourse.coursecategory</field>
        <field name="arch" type="xml">
            <tree string="kategori">
                <field name="name"/>
                <field name="level_belajar"/>
                <field name="kapasitas_kelas"/>
                <field name="biaya"/>
            </tree> 
        </field>
   </record>

   <record id="wikucourse_coursecategory_view_form" model="ir.ui.view">
       <field name="name">wikucourse_coursecategory_view_form</field>
       <field name="model">wikucourse.coursecategory</field>
       <field name="arch" type="xml">
           <form string="wikucourse_coursecategory_form">
               <sheet>
                    <group name="name" string="Nama Kursus">
                       <field name="name"/>
                    </group>
                    <group name="name" string="Detail Kursus">
                       <field name="level_belajar"/>
                       <field name="kapasitas_kelas"/>
                       <field name="biaya"/>
                    </group>                    
               </sheet>
           </form>
       </field>
   </record>
    
   
</odoo>